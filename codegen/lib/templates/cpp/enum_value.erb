<%  has_string = entity.cases.all? { |c| !c.string.nil? } -%>
<%  wasm_class_name = WasmCppHelper.class_name(entity: entity) -%>
<%  if has_string -%>
std::string <%= wasm_class_name %>::description() const {
    switch (instance) {
<%      entity.cases.each do |c| -%>
        case TW<%= entity.name %><%= c.name %>: return <%= c.string %>;
<%      end -%>
        default: return "";
    }
}
<%  end-%>
